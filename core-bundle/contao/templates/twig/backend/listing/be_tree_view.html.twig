{% extends '@Contao/backend/listing/be_listing.html.twig' %}

{% block content %}
    {{ help ?? ''|raw }}
    {{ breadcrumbs ?? ''|raw }}
    {{ globalOperations ?? ''|raw }}

    <ul {{ wrapper_attributes ?? '' }}>
        <li class="tl_folder_top cf">
            <div class="tl_left"></div>
            <div class="tl_right">
                {{ operations ?? '' }}
            </div>
        </li>
        {{ children ?? ''|raw }}
    </ul>

    {{ reset ?? ''|raw }}

    {% if not children ?? true %}
        {{ include('@Contao/backend/listing/be_empty.html.twig', {label: emptyLabel}) }}
    {% endif %}
{% endblock %}
