<{{ tagName }}>
{%- block inner %}
    {%- apply spaceless %}
        {% if as_editor_view %}
            {% block overview %}{{ text|striptags }}{% endblock %}
        {% else %}
            {% block headline %}{{ text|insert_tag_raw }}{% endblock %}
        {% endif %}
    {% endapply -%}
{% endblock -%}
</{{ tagName }}>
