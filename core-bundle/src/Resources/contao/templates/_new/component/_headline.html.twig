<{{ headline.tagName }}>
{%- block inner %}
    {%- apply spaceless %}
        {% if as_overview %}
            {% block overview %}{{ headline.text|striptags }}{% endblock %}
        {% else %}
            {% block headline %}{{ headline.text|insert_tag_raw }}{% endblock %}
        {% endif %}
    {% endapply -%}
{% endblock -%}
</{{ headline.tagName }}>
